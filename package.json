{
  "name": "ezretro.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "firebase": "^7.3.0",
    "v-clipboard": "^2.2.2",
    "vue": "^2.6.10",
